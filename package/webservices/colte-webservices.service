[Unit]
Description=CoLTE WebAdmin
After=mysql.service
Requires=mysql.service

[Service]
ExecStart=/usr/bin/colte-webservices/bin/www
Restart=always
RestartSec=5
Environment=PATH=/usr/bin:/usr/local/bin
Environment=NODE_ENV=production
WorkingDirectory=/usr/bin/colte-webservices

[Install]
WantedBy=multi-user.target
